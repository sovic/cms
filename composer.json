{
    "name": "sovic/cms",
    "description": "Web applications SDK",
    "keywords": [
    ],
    "type": "library",
    "autoload": {
        "psr-4": {
            "Sovic\\Cms\\": "src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Sovic\\Cms\\Tests\\": "tests/"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/sovic/common.git"
        },
        {
            "type": "git",
            "url": "https://github.com/sovic/common-ui.git"
        },
        {
            "type": "git",
            "url": "https://github.com/sovic/gallery.git"
        },
        {
            "type": "git",
            "url": "https://github.com/sovic/user-bundle.git"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.2",
        "ext-imagick": "*",
        "ext-intl": "*",
        "cocur/slugify": "^4.0",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/orm": "^2.10",
        "sovic/common": "dev-master",
        "sovic/gallery": "dev-master",
        "symfony/form": "^7.3",
        "symfony/framework-bundle": "^7.3",
        "symfony/http-foundation": "^7.3",
        "symfony/mailer": "^7.3",
        "symfony/security-bundle": "^7.3",
        "symfony/twig-bundle": "^7.3",
        "symfony/validator": "^7.3",
        "sovic/common-ui": "dev-master",
        "sovic/user-bundle": "^0.2.18"
    },
    "require-dev": {
        "rector/rector": "^1.0",
        "roave/security-advisories": "dev-latest"
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
