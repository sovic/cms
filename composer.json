{
    "name": "sovic/symfony-cms",
    "description": "Symfony web applications SDK",
    "keywords": [
    ],
    "type": "library",
    "autoload": {
        "psr-4": {
            "Sovic\\Cms\\": "src/"
        },
        "classmap": [
            "src/"
        ]
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/sovic/symfony-gallery.git"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "require": {
        "php": "^8.0",
        "cocur/slugify": "^4.0",
        "doctrine/annotations": "^1.13",
        "doctrine/doctrine-bundle": "^2.7",
        "doctrine/orm": "^2.10",
        "symfony/form": "^6.0",
        "symfony/framework-bundle": "^6.0",
        "symfony/http-foundation": "^6.0",
        "symfony/mailer": "^6.0",
        "symfony/security-bundle": "^6.0",
        "symfony/twig-bundle": "^6.0",
        "symfony/validator": "^6.0",
        "sovic/symfony-gallery": "^0.2.2"
    },
    "require-dev": {
    },
    "config": {
        "allow-plugins": {
            "composer/package-versions-deprecated": true
        }
    }
}
